corr <- function(directory, threshold = 0) {
corr <- function(directory, threshold = 0) {
## 'directory' is a character vector of length 1 indicating
## the location of the CSV files
## 'threshold' is a numeric vector of length 1 indicating the
## number of completely observed observations (on all
## variables) required to compute the correlation between
## nitrate and sulfate; the default is 0
folder=directory
## Return a numeric vector of correlations
c_d = complete("folder")
## Above threshold
ID = c_d[c_d$nobs > threshold, ]$id
row=181
calculate.cor = function(row) {
coca = complete.cases(row)
cor(row[coca , ]$sulfate, row[coca, ]$nitrate)
}
above.threshold = lapply(ID, getmonitor, directory = directory)
sapply(above.threshold, calculate.cor)
}
